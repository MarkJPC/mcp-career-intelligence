{
  "name": "@mark-cena/mcp-career-intelligence-server",
  "version": "1.0.0",
  "description": "Production-ready MCP server for career intelligence platform with Notion integration",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "docker:build": "docker build -t mcp-career-server .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mcp-career-server"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "career-intelligence",
    "notion-api",
    "typescript",
    "backend-engineering"
  ],
  "author": "Mark Cena <mark@example.com>",
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "node-cache": "^5.1.2",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mark-cena/mcp-career-intelligence.git",
    "directory": "server"
  },
  "bugs": {
    "url": "https://github.com/mark-cena/mcp-career-intelligence/issues"
  },
  "homepage": "https://github.com/mark-cena/mcp-career-intelligence#readme"
}